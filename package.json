{
  "name": "@konnec/vue-middleware-plugin",
  "version": "0.1.3",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.1",
    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@konnec/vue-eloquent": "^1.0.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/ui": "^0.34.6",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "description": "A middleware plugin for Vue 3",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-middleware-plugin.git"
  },
  "keywords": [
    "Vue",
    "Model",
    "Controller",
    "Middleware",
    "Router",
    "Routing"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-middleware-plugin/issues"
  },
  "homepage": "https://github.com/cimd/vue-middleware-plugin#readme"
}
