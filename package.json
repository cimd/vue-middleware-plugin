{
  "name": "@konnec/vue-middleware-plugin",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.json --composite false",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write src/ test/"
  },
  "description": "A middleware plugin for Vue 3",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "dependencies": {
    "path-to-regexp": "^8.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@konnec/vue-eloquent": "^1.4.4",
    "@rushstack/eslint-patch": "^1.10.4",
    "@stylistic/eslint-plugin-js": "^2.12.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "^1.1.20",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-middleware-plugin.git"
  },
  "keywords": [
    "Vue",
    "Model",
    "Controller",
    "Middleware",
    "Router",
    "Routing"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-middleware-plugin/issues"
  },
  "homepage": "https://github.com/cimd/vue-middleware-plugin#readme"
}
